    <script>
            jq(document).ready(function() {
                jq(function() {
                    if (jq( "#from" ).length > 0 && jq( "#to" ).length > 0) {
                      jq( "#from" ).datepicker({
                          defaultDate: "+1w",
                          showOn: "button",
                          buttonImage: "/assets/images/generic/website/calendar.jpg",
                          buttonImageOnly: true,
                          changeMonth: true,
                          numberOfMonths: 1,
                          onSelect: function( selectedDate ) {
                              jq( "#to" ).datepicker( "option", "minDate", selectedDate );
                          }
                      });
                      jq( "#to" ).datepicker({
                          showOn: "button",
                          buttonImage: "/assets/images/generic/website/calendar.jpg",
                          buttonImageOnly: true,
                          defaultDate: "+1w",
                          changeMonth: true,
                          numberOfMonths: 1,
                          onSelect: function( selectedDate ) {
                              jq( "#from" ).datepicker( "option", "maxDate", selectedDate );
                          }
                      });
                   }
                });
            });
            
            // Initialize scripts referenced in the plugins.js that need to be actioned from
            // here to allow access to the document.body tag attributes
            initReadMoreButton ();
            initContinueReading();
             
    </script>
	


